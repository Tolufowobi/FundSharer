@using FundSharer.Models;
@model RegisterViewModel 

    <dl class="text-info">
        <dt>@Html.Label("First Name")</dt>
        <dd><div id="uFirstName">@Html.TextBoxFor(m => Model.FirstName)</div></dd>
        <dt>@Html.Label("Last Name")</dt>
        <dd><div id="uLastName">@Html.TextBoxFor(m => Model.LastName)</div></dd>
        <dt>@Html.Label("Email Address")</dt>
        <dd><div id="uEmailAddress">@Html.TextBoxFor(m => Model.Email)</div></dd>
        <dt>@Html.Label("Phone Number")</dt>
        <dd><div id="uPhoneNumber">@Html.TextBoxFor(m => Model.ContactNumber)</div></dd>
        
    </dl>
    <hr />
    <div id="uButtons" class="EditButtons">
        <button id="uButtonSubmit" class="btn btn-success" onclick="SaveChanges(uButtonSubmit)" hidden="hidden">Save</button>
        <button id="uButtonCancel" class="btn btn-danger" onclick="Cancel(uButtonCancel)" hidden="hidden">Cancel</button>
    </div>


